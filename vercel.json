{
  "routes": [
    { "src": "/api/login", "dest": "/src/loginRouter.js" },
    { "src": "/api/register", "dest": "/src/registerRouter.js" },
    { "src": "/api/products", "dest": "/src/productRouter.js" }
  ],
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/$1"
    }
  ],
  "builds": [
    {
      "src": ".vercel/reset_script.js",
      "use": "@vercel/node"
    }
  ]
}
